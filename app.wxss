.container {
  width:100vw;
  display:flex;
  flex-direction:column;
  box-sizing:border-box;
  overflow:hidden;
}
.i-title{
  border-left:3px solid #0c8;
  padding-left:6px;
  font-size:15px;
  font-weight:500;
}
.pt-content{
  padding:12px 10px;
  display:flex;
  justify-content:space-between;
  box-sizing:border-box;
}
.pt-list{
  margin-top:10px;
}
.pt-panel{
  margin-top:10px;
  background:#fff;
}
.loading{
  animation: loading 1.5s 0s linear infinite
}
@keyframes loading{
  100%{
    transform: rotate(360deg)
  }
}
.scroll{
  height:120px;
  display:flex;
  white-space:nowrap;
  box-sizing:border-box;
}

.no-data{
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 14px;
  color: #888;
  background: #f6f6f6;
  position:fixed;
  top:42px;
  left:0;
  right:0;
  bottom:0;
}
.no-data-icon{
  width:96px;
  height:96px;
  background-size:100%;
  background-repeat: no-repeat;
  position:absolute;
  top:40%;
  left:50%;
  transform: translate(-50%,-50%);
  background-image: url("data:image/png;base64,");
}