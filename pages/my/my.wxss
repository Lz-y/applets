.container{
  background:#f6f6f6;
  height:100vh;
}
.y-user{
  font-size:14px;
  color:#444;
}
.user-info{
  height:90px;
  display:flex;
  align-items:center;
  padding:0 20px;
  margin-top: 10px;
  background: #fff;
  box-sizing:border-box;
  position: relative;
}
.user-avatar{
  width:60px;
  height:60px;
  overflow:hidden;
  border-radius: 6px;
}
.user-nickName-school{
  display:flex;
  flex-direction:column;
  margin-left:15px;
  line-height:1.5;
}
.enter{
  position:absolute;
  top:50%;
  right:0%;
  transform:translate(-50%, -50%);
}
.pt-item{
  width:50%;
  display:flex;
  flex-direction:column;
  justify-content:center;
  align-items:center;
}
.pt-content{
  padding:12px 16px;
}
.n-user{
  display:flex;
  flex-direction:column;
  justify-content:center;
  font-size:14px;
  text-align:center;
  width:100vw;
  height:100vh;
  color:#333;
}
.apply, .record{
  width:42px;
  height:42px;
  background-size:100%;
  background-repeat:no-repeat;
}
.apply{
  background-image: url("data:image/png;base64,");
}
.record {
  background-image: url("data:image/png;base64,");
}
.contact {
  width: 40px;
  height: 40px;
  z-index: 99;
  border-radius: 50%;
  background-color: #0c8;
  position: fixed;
  bottom:40px;
  right:16px;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
}